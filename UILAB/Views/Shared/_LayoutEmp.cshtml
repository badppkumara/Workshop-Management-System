        </div>
        <div class="layout-content">
            @RenderBody()
        </div>

        <div class="layout-footer">
            <div class="layout-footer-body">
                <small class="version">Version @Session["BuildVersion"]</small>
                <small class="copyright">@DateTime.Now.Year &copy; Developed By <a href="#">System Solutions</a></small>
            </div>
        </div>
