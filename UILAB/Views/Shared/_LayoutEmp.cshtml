    <div class="layout-main">
        <div class="layout-sidebar">
            <div class="layout-sidebar-backdrop"></div>
            <div class="layout-sidebar-body">
                <div class="custom-scrollbar">
                    <nav id="sidenav" class="sidenav-collapse collapse">
                        <ul class="sidenav">
                            <li class="sidenav-heading">Main</li>
                            <li class="sidenav-item @(ViewBag.Current == "Dashboard" ? "active" : "")">
                                <a href="~/Emp/Dashboard">
                                    <span class="sidenav-icon icon icon-home"></span>
                                    <span class="sidenav-label">Dashboard</span>
                                </a>
                            </li>

                            <li class="sidenav-item has-subnav @(ViewBag.Current == "User" ? "active open" : "")">
                                <a href="#" aria-haspopup="true">
                                    <span class="sidenav-icon icon icon-users"></span>
                                    <span class="sidenav-label">User Management</span>
                                </a>
                                <ul class="sidenav-subnav collapse">
                                    <li class="sidenav-subheading">User Management</li>
                                    <li class="@(ViewBag.CurrentSub == "Customer" ? "active" : "")">@Html.ActionLink("Customers", "List", "EmpCustomer")</li>
                                    <li class="@(ViewBag.CurrentSub == "Supplier" ? "active" : "")">@Html.ActionLink("Suppliers", "List", "EmpSupplier")</li>
                                </ul>
                            </li>

                            <!-- Vehicle Management -->
                            <li class="sidenav-heading">Vehicle Management</li>
                            <li class="sidenav-item has-subnav @(ViewBag.Current == "Vehicle" ? "active open" : "")">
                                <a href="#" aria-haspopup="true">
                                    <span class="sidenav-icon icon icon-truck"></span>
                                    <span class="sidenav-label">Vehicles</span>
                                </a>
                                <ul class="sidenav-subnav collapse">
                                    <li class="sidenav-subheading">Vehicles</li>
                                    <li class="@(ViewBag.CurrentSub == "Make" ? "active" : "")">@Html.ActionLink("Make", "List", "EmpVehicleMake")</li>
                                    <li class="@(ViewBag.CurrentSub == "Model" ? "active" : "")">@Html.ActionLink("Model", "List", "EmpVehicleModel")</li>
                                    <li class="@(ViewBag.CurrentSub == "Vehicles" ? "active" : "")">@Html.ActionLink("Vehicles", "List", "EmpVehicles")</li>
                                </ul>
                            </li>

                            <!-- Workshop Management -->
                            <li class="sidenav-heading">Workshop Management</li>
                            <li class="sidenav-item has-subnav @(ViewBag.Current == "Jobs" ? "active open" : "")">
                                <a href="#" aria-haspopup="true">
                                    <span class="sidenav-icon icon icon-edit"></span>
                                    <span class="sidenav-label">Jobs</span>
                                </a>
                                <ul class="sidenav-subnav collapse">
                                    <li class="sidenav-subheading">Jobs</li>
                                    @*<li class="@(ViewBag.CurrentSub == "Inspect" ? "active" : "")">@Html.ActionLink("Inspections", "List", "Inspection")</li>
