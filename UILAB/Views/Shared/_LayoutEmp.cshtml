                            <!-- Stock Management -->
                            <li class="sidenav-heading">Stock Management</li>
                            <li class="sidenav-item has-subnav @(ViewBag.Current == "Stock" ? "active open" : "")">
                                <a href="#" aria-haspopup="true">
